{
  "name": "ngx-smart-popover-demo",
  "version": "1.5.0",
  "resolutions": {
    "normalize-url": "6.1.0",
    "serialize-javascript": "6.0.0",
    "trim-newlines": "3.0.1"
  },
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:all": "ng build && npm run build:lib",
    "build:lib": "ng build ngx-smart-popover && npm run build:lib:scss && npm run build:lib:css",
    "build:lib:css": "sass ./projects/ngx-smart-popover/src/lib/popover-content.component.scss ./dist/ngx-smart-popover/css/popover-content.component.css",
    "build:lib:scss": "mkdir ./dist/ngx-smart-popover/scss/ && cp ./projects/ngx-smart-popover/src/lib/popover-content.component.scss ./dist/ngx-smart-popover/scss/popover-content.component.scss",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:entrypoint": "npm install && ng e2e",
    "e2e:docker": "docker build -t ngx-smart-popover-e2e-tests . && docker run -v ~/code/ngx-smart-popover:/usr/src/app --rm ngx-smart-popover-e2e-tests",
    "run": "ng-packagr -p package.json",
    "publish": "cp README.md dist/ngx-smart-popover/README.md && cd dist/ngx-smart-popover && npm publish"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.4.0",
    "@angular/common": "13.4.0",
    "@angular/compiler": "13.4.0",
    "@angular/core": "^13.4.0",
    "@angular/forms": "13.4.0",
    "@angular/platform-browser": "13.4.0",
    "@angular/platform-browser-dynamic": "13.4.0",
    "@angular/router": "13.4.0",
    "bootstrap": "4.3.1",
    "core-js": "2.6.4",
    "rxjs": "6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.11",
    "@angular/cli": "^13.3.11",
    "@angular/compiler-cli": "13.4.0",
    "@angular/language-service": "13.4.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^13.3.1",
    "normalize-url": "6.1.0",
    "protractor": "~7.0.0",
    "protractor-jasmine2-screenshot-reporter": "0.5.0",
    "protractor-screenshot-extension": "1.0.6",
    "sass": "^1.51.0",
    "scss-bundle": "3.1.2",
    "serialize-javascript": "6.0.0",
    "trim-newlines": "3.0.1",
    "ts-node": "7.0.1",
    "tslint": "~6.1.0",
    "typescript": "4.6.4"
  }
}